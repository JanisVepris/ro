{% spaceless %}
<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>
        MIDI RO ADMIN {% if tabTitle is defined %} - {{ tabTitle }}{% endif %}
    </title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('bundles/admin/css/materialize.min.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/admin/js/filer/css/jquery.filer.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/admin/js/filer/css/themes/jquery.filer-dragdropbox-theme.css') }}">

    {% if sidebar is not defined or sidebar %}
        <link rel="stylesheet" href="{{ asset('bundles/admin/css/custom.css') }}">
    {% endif %}
</head>
<body>

{% include '@Admin/AdminLayout/header.html.twig' %}

<main>
    <div class="container">
        {% block content %}{% endblock %}
    </div>
</main>

<script src="{{ asset('bundles/admin/js/jquery-3.1.1.min.js') }}"></script>
<script src="{{ asset('bundles/admin/js/materialize.min.js') }}"></script>
<script src="{{ asset('bundles/admin/js/filer/js/jquery.filer.min.js') }}"></script>
</body>
</html>
{% endspaceless %}
